{
  "compilerOptions": {
    "target": "ESNext",
    "incremental": true,
    "declaration": true,
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "removeComments": true
  },
  "include": [
    "./types"
  ]
}
